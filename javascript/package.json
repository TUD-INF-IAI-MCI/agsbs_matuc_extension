{
    "name": "agsbs",
    "displayName": "AGSBS",
    "description": "This package is developed to make the transcription process more effective of our working group",
    "version": "0.0.1",
    "publisher": "absbs",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.agsbs",
        "onLanguage:Markdown",
        "onLanguage:markdown"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.agsbs",
                "title": "Opening AGSBS Panel"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "eslint": "^4.11.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
